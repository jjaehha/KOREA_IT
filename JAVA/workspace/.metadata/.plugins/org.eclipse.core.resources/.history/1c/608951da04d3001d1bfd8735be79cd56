package project;

import java.util.Random;

public class Lotto {
	public static void main(String[] args) {
		int lottoNum = 0;
		
		Random ran = new Random();
		lottoNum = (ran.nextInt(45)+1);
		
		for (int i=0; i<6; i++) {
			System.out.println("로또번호는" + lottoNum + "입니다");
		}
	}

}
