package day07;
class Tv{
   int ch;
   int vol;
   boolean power;
   int inch;
   int price;
   String color;
   public Tv() {
   }
   public Tv(int inch, int price, String color) {
      this.inch = inch;
      this.price = price;
      this.color = color;
   }
   public Tv(int inch, int price, int ch) {
      this.inch = inch;
      this.price = price;
      this.ch = ch;
   }
   void powerOnOff() {}
   void ChUp() {}
   void ChDown() {}
   
}
